<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>FantasyLeague</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>UserLogin</display-name>
    <servlet-name>UserLogin</servlet-name>
    <servlet-class>com.cts.nfl.contollers.UserLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PredictRedirect</display-name>
    <servlet-name>PredictRedirect</servlet-name>
    <servlet-class>com.cts.nfl.contollers.PredictRedirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PredictRedirect</servlet-name>
    <url-pattern>/PredictRedirect</url-pattern>
  </servlet-mapping>
</web-app>